package pack01;

import javax.swing.JButton;
import javax.swing.JFrame;

public class MyWindow {

	public static void main(String[] args) {
		// 필요한 부품들 정리
		// 전체 틀
		JFrame f = new JFrame();
		// 버튼 2개
		JButton b1 = new JButton();
		JButton b2 = new JButton();
		// 글자
		// 색
		
		f.add(b1);
		f.setVisible(true);
	}

}
